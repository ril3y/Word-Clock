;*****************************************************************************
; PIC18F family - cues and playback data file
;*****************************************************************************
;
;    Filename:	    	eFruit_cue_data.inc
;    Author, Company:	Alden Hart, Luke's Lights
;    Chip Support:	Supports PIC18F family
;    Revision:		090711
;
; TABLE STRUCTURE:
;     PLAYLIST_MASTER_TABLE	List of playlists - one or more playlists
;				Can be selected by:
; 				  	PLAYLIST command (run numbered playlist) 
;					PLAY_NEXT_PLAYLIST (end of previous playlist)
;					PLAY_PREV_PLAYLIST
;
;     PLAYLIST_CUE_NUMBER_TABLE	Set of numbered cues for calling from commands
;				Can be selected by:
; 				  	CUE command (run numbered cue) 
;
;     PLAYLIST_xxxxxx		Individual playlists - one or more cues each
;				Advances by WATCH command, does LOOPING
;
;     CUE_xxxxxx		Individial cues - one or more commands each

DATA_LED_DATA_TABLES	code	; set to align on 16 bit word boundaries

;******************************************************************************
; PLAYLIST MASTER TABLE
; Points to one or more playlists. 

	data	PLAYLIST_DONE	; REQUIRED
PLAYLIST_MASTER_TABLE
	data	PLAYLIST_01	; This will get executed on power-up reset
	data	PLAYLIST_02
	data	PLAYLIST_DONE	; REQUIRED


;******************************************************************************
; CUE NUMBER TABLE
; A playlist containing cues that rae accessed by nuymber using the CUE command
; The first cue is #00 and is ALL OFF by convention

CUE_NUMBER_TABLE
	data	PLAYLIST_DONE	; REQUIRED

;---- mapping to this table
_ALL_OFF	equ	.00
_ALL_ON		equ	.01
_ALL_OFF_XFADE	equ	.02
_ALL_ON_XFADE	equ	.03

;******************************************************************************
; PLAYLIST TABLES

PLAYLIST_01
;	data	CUE_ALL_ON
	data	CUE_ALL_DIM
;	data	CUE_ALL_OFF
	data	PLAYLIST_DONE	; REQUIRED

PLAYLIST_02
	data	CUE_ALL_OFF
	data	PLAYLIST_DONE	; REQUIRED

